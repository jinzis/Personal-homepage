{
  "extends": "plugin:jsx-a11y/recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "strict": [2, "global"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "$": true,
    "window": true,
    "document": true,
    "__dirname": true
  },
  "parser":"babel-eslint",
  "plugins": [
     "jsx-a11y"
  ],
  "rules": {
    "jsx-a11y/onclick-has-focus": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/onclick-has-role": 0,
    "no-new-func": 1,
    "func-names": 0,
    "no-new": 0,
    "no-param-reassign": [2, { "props": false }],
    "yoda": [1, "never", {"exceptRange": true,"onlyEquality": true}],
    "no-useless-concat": 0,
    "no-implicit-coercion": [2, {
            "boolean": true,
            "number": true,
            "string": true,
            "allow": [/* "!!", "~", "*", "+" */]
        }],
    "semi": [2, "never"]
  }
}

